<header>
    <div class="container">
        <div class="header-top">
            <div class="row">
                <div class="col-md-3">
                    @await Component.InvokeAsync("Logo")
                </div>
                <div class="col-md-6">
                    @await Component.InvokeAsync("SearchBox")
                </div>
                <div class="col-md-3">
                    @await Component.InvokeAsync("HeaderLinks")
                </div>
            </div>
        </div>
        <div class="header-bottom">
            @await Component.InvokeAsync("TopMenu")

        </div>
    </div>
</header>
