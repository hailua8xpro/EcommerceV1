@model WardModel

@{
    ViewBag.Title = T("Admin.Configuration.Countries.Wards.AddNew").Text;
}

<form asp-controller="Country" asp-action="DistrictCreate"
      asp-route-stateProvinceId="@Context.Request.Query["districtId"]"
      asp-route-btnId="@Context.Request.Query["btnId"]"
      asp-route-formId="@Context.Request.Query["formId"]">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Admin.Configuration.Countries.Wards.AddNew")
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdateWard", Model)
</form>